{
    "app_name": "Foo App",
    "app_package": "tkotosz/fooapp-src",
    "app_version": "0.2.0",
    "app_dir": ".fooapp",
    "app_factory": "Tkotosz\\FooApp\\ApplicationFactory",
    "app_extensions": {
        "package_type": "tkotosz-fooapp-extension",
        "extension_class_config_field": "tkotosz-fooapp-extension-class"
    },
    "global_mode_enabled": true,
    "local_mode_enabled": true,
    "local_working_directory_resolvers": ["git", "cwd"],
    "app_executable_name": "fooapp",
    "github_user": "tkotosz",
    "github_repository": "fooapp",
    "build_target_dir": "bin"
}
